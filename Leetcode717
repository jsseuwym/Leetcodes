class Solution {
public:
    bool isOneBitCharacter(vector<int>& bits) {
        bool ch;
        int i;
        for(i=0;i<bits.size();i++)
        {
            if(bits[i]==1)
            {
                i++;
                ch=false;
            }
            else
            {
                ch=true;
            }
        }
        return ch;
    }
};
